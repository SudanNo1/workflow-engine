<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo
       http://code.alibabatech.com/schema/dubbo/dubbo.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <context:property-placeholder location="config.properties"/>
    <dubbo:application name="Workflow-Engine-Provider"/>
    <!--zookeeper注册中心 -->
    <dubbo:registry id="wf-provider" protocol="zookeeper" address="${dubbo.zookeeper}"/>

    <dubbo:protocol port="28880"/>

    <dubbo:reference  interface="org.radrso.demo.HelloWorld" id="helloWorldImpl"></dubbo:reference>
    <dubbo:reference  interface="org.radrso.demo.test.Test1" id="test1Impl"></dubbo:reference>
    <dubbo:reference  interface="org.radrso.demo.test.Test2" id="test2Impl"></dubbo:reference>


</beans>